{
    "name": "klage-dittnav",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start:js": "react-scripts start",
        "build:js": "react-scripts build",
        "start:css": "less-watch-compiler --config less.json",
        "build:css": "less-watch-compiler --run-once --config less.json",
        "clean-build": "rimraf ./build",
        "docker-build": "npm run clean-build && npm run build && docker build . -t klage-dittnav",
        "docker-start": "docker run --name klage-dittnav -p 8080:8080 -d klage-dittnav",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm klage-dittnav",
        "docker-stop": "docker stop klage-dittnav",
        "docker-rmi": "npm run docker-kill && docker rmi klage-dittnav",
        "start": "npm-run-all -p start:*",
        "build": "npm-run-all -p build:*",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prettier": "prettier --write src/**/*.{tsx,ts,less}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "setupFiles": [
        "src/setupTests.ts"
    ],
    "dependencies": {
        "@navikt/fnrvalidator": "^1.1.3",
