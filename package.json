        "@types/amplitude-js": "^8.9.3",
        "@types/detect-browser": "^3.0.0",
        "@types/express": "^4.17.13",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.9",
        "@types/react": "^17.0.38",
