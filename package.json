{
    "name": "klage-dittnav",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "copy-pdf-js": "node ./scripts/copy-pdf-js.js",
        "prestart": "npm run copy-pdf-js",
        "prebuild": "npm run copy-pdf-js",
        "start:js": "react-scripts start",
        "build:js": "react-scripts build",
        "start:css": "less-watch-compiler --config less.json",
        "build:css": "less-watch-compiler --run-once --config less.json",
        "clean-build": "rimraf ./build",
        "docker-build": "npm run clean-build && npm run build && docker build . -t klage-dittnav",
        "docker-start": "docker run --name klage-dittnav -p 8080:8080 -d klage-dittnav",
        "docker-kill": "npm run docker-stop && npm run docker-rm",
        "docker-rm": "docker rm klage-dittnav",
        "docker-stop": "docker stop klage-dittnav",
        "docker-rmi": "npm run docker-kill && docker rmi klage-dittnav",
        "start": "npm-run-all -p start:*",
        "build": "npm-run-all -p build:*",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prettier": "prettier --write src/**/*.{tsx,ts,less}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "setupFiles": [
        "src/setupTests.ts"
    ],
    "dependencies": {
        "@navikt/ds-css": "^0.12.11",
        "@navikt/ds-icons": "^0.7.2",
        "@navikt/ds-react": "^0.14.11",
        "@navikt/fnrvalidator": "^1.1.4",
        "@navikt/nav-dekoratoren-moduler": "^1.6.9",
        "amplitude-js": "^8.15.0",
        "classnames": "^2.3.1",
        "dayjs": "^1.10.7",
        "detect-browser": "^5.3.0",
        "forhandsvisningsfil": "^2.1.2",
        "lodash": "^4.17.21",
        "lodash.throttle": "^4.1.1",
        "nav-datovelger": "^12.1.2",
        "nav-frontend-alertstriper": "^4.0.1",
        "nav-frontend-alertstriper-style": "^3.0.1",
        "nav-frontend-ekspanderbartpanel": "^4.0.3",
        "nav-frontend-ekspanderbartpanel-style": "^2.0.1",
        "nav-frontend-knapper": "^3.1.2",
